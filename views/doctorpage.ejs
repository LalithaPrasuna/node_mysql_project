<!DOCTYPE>
<html ng-app="myApp">

<head>
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css">

    <!-- Optional theme -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap-theme.min.css">

    <title>CLINIC</title>
    
</head>
    

<body><script src="ejs.js"></script>
    <div class="container" ng-controller="AppCtrl">
        <h1>hi!! <%= username[0].dname_name %></h1>
        <h2><%= username[0].Specialisation  %></h2>
        <p><%= username[0].Experience  %></p>
        <p><%= username[0].Address %></p>
        <p><%= username[0].Age %></p>
        <p><%= username[0].City %></p>
        
        
        

        <table class="table">
            
            <tbody>
                <tr>
                <td>PatientName</td>
                
                <td>PetName</td>
                
                <td>DoctorName</td>
                
                <td>Reason</td>
                
                <td>prescription</td>
                </tr>
                <tr>
                    <% for(var i=0; i<username.length; i++) {%>
   
   <td><%= username[i].owner_name %></td>
   <td><%= username[i].pet_name %></td>
   <td><%= username[i].reason %></td>
   <td><%= username[i].prescription  %></td>
                    
<% } %>
                </tr>
            </tbody>
        </table>
        
        <a href="/seeappointments?id=<%= username[0].dname_name %>" class="btn btn-success" >See all the appointment</a>

    </div>


    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.12/angular.min.js"></script>
    <script src="controllers/controller.js"></script>
    <script src="js/jquery-1.10.2.js"></script>
<script>
    var myvar = <%- JSON.stringify(username) %>;
    console.log(myvar);
    </script>
</body>

</html>
